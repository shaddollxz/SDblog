    "version": "1.0.0",
    "packageManager": "pnpm@7.1.9",
    "description": "",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "start": "sh ./scripts/sh/startBlog.sh",
        "dev:client": "cd packages/client && pnpm dev",
        "dev:server": "cd packages/server && pnpm dev",
        "build:client": "cd packages/client && pnpm build",
        "build:client:dev": "sh ./scripts/sh/buildClientDev.sh",
        "build:server": "sh ./scripts/sh/buildServer.sh",
        "preview:client": "cd packages/client && pnpm preview",
        "preview:server": "cd packages/server && pnpm preview",
        "test": "vitest",
        "formatSVG": "cd ./scripts && pnpm formatSVG",
        "clearlogs": "pm2 flush",
        "exportDB": "sh ./scripts/sh/exportDB.sh",
        "importDB": "sh ./scripts/sh/importDB.sh",
        "test:all": "vitest run",
        "lint:lint-staged": "lint-staged"
