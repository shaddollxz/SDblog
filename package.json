        "jsdom": "^20.0.0",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "tsx": "^3.8.0",
        "typescript": "^4.7.4",
        "vitest": "^0.18.0"
    },
    "dependencies": {
        "axios": "^0.24.0",
